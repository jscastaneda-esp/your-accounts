{
	"name": "your-accounts",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "svelte-kit sync && vitest run --outputFile=./test-results.json --reporter=json --coverage",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@bdsqqq/try": "2.3.1",
		"@felte/validator-yup": "1.1.3",
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/kit": "1.24.1",
		"@trpc/client": "10.38.1",
		"@trpc/server": "10.38.1",
		"@types/jsonwebtoken": "9.0.2",
		"@typescript-eslint/eslint-plugin": "6.6.0",
		"@typescript-eslint/parser": "6.6.0",
		"@vitest/coverage-v8": "0.34.3",
		"autoprefixer": "10.4.15",
		"boxicons": "2.1.4",
		"chart.js": "4.4.0",
		"daisyui": "3.6.5",
		"dayjs": "1.11.9",
		"delay": "6.0.0",
		"eslint": "8.48.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-svelte": "2.33.0",
		"felte": "1.2.12",
		"firebase": "10.3.1",
		"jsonwebtoken": "9.0.2",
		"postcss": "8.4.29",
		"prettier": "3.0.3",
		"prettier-plugin-svelte": "3.0.3",
		"superjson": "1.13.1",
		"svelte": "4.2.0",
		"svelte-chartjs": "3.1.2",
		"svelte-check": "3.5.1",
		"svelte-sonner": "0.1.4",
		"tailwindcss": "3.3.3",
		"trpc-sveltekit": "3.5.20",
		"tslib": "2.6.2",
		"typescript": "5.2.2",
		"vite": "4.4.9",
		"vitest": "0.34.3",
		"yup": "1.2.0",
		"zod": "3.22.2"
	},
