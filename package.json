{
	"name": "your-accounts",
	"version": "1.2.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "svelte-kit sync && vitest run --outputFile=./test-results.json --reporter=json --coverage",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@auth/sveltekit": "1.1.0",
		"@bdsqqq/try": "2.3.1",
		"@felte/validator-yup": "1.1.3",
		"@sveltejs/adapter-auto": "2.1.1",
		"@sveltejs/kit": "1.30.4",
		"@trpc/client": "10.45.2",
		"@trpc/server": "10.45.2",
		"@typescript-eslint/eslint-plugin": "6.12.0",
		"@typescript-eslint/parser": "6.12.0",
		"autoprefixer": "10.4.19",
		"chart.js": "4.4.3",
		"daisyui": "4.11.1",
		"dayjs": "1.11.11",
		"delay": "6.0.0",
		"eslint": "8.54.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.39.0",
		"felte": "1.2.14",
		"postcss": "8.4.38",
		"prettier": "3.2.5",
		"prettier-plugin-svelte": "3.2.3",
		"superjson": "2.2.1",
		"svelte": "4.2.7",
		"svelte-chartjs": "3.1.2",
		"svelte-check": "3.7.1",
		"svelte-sonner": "0.3.24",
		"tailwindcss": "3.4.3",
		"trpc-sveltekit": "3.6.1",
		"tslib": "2.6.2",
		"typescript": "5.2.2",
		"vite": "4.5.3",
		"yup": "1.4.0",
		"zod": "3.23.8"
	},
	"type": "module",
	"overrides": {
		"svelte-chartjs": {
			"svelte": "4.2.7"
		},
		"trpc-sveltekit": {
			"@sveltejs/adapter-node": "1.3.1"
		}
	},
	"dependencies": {
		"ofetch": "1.3.3"
	}
}
